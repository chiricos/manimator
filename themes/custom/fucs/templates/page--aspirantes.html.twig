                <p><img src="/fucsalud/themes/custom/fucs/images/iconoVideo.png" alt="">VIDEOS</p>
            </div>
            <div class="calendar-content">
                {{ page.video }}
            </div>
        </div>
    </div>
</div>



<div class="form-inscribirse" style="display: none;">
    <span>X</span>
    {{ page.form_contact }}
</div>


