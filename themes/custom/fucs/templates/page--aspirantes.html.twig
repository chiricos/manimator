                    <a class="active menu-eventos" id="calendario">CALENDARIO DE INDUCCIONES</a>
                    <!--<a class="menu-encuestas" id="menu-encuestas">LISTA DE ADMITIDOS</a>-->
                    <!-- <a class="menu-videos" id="menu-videos">VIDEO</a> -->
                </div>

                <div class="sub-eventos">
